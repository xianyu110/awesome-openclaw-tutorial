<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          apparmorProfile</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;openclaw-sandbox&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          dns</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;1.1.1.1&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;8.8.8.8&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          extraHosts</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;internal.service:10.0.0.5&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          binds</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;/var/run/docker.sock:/var/run/docker.sock&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;/home/user/source:/source:rw&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">        },</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        browser</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          enabled</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">false</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          image</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;openclaw-sandbox-browser:bookworm-slim&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          containerPrefix</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;openclaw-sbx-browser-&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          cdpPort</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#D4D4D4">9222</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          vncPort</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#D4D4D4">5900</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          noVncPort</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#D4D4D4">6080</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          headless</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">false</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          enableNoVnc</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">true</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          allowHostControl</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">false</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          allowedControlUrls</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;http://10.0.0.42:18791&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          allowedControlHosts</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;browser.lab.local&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;10.0.0.42&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          allowedControlPorts</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span><span style="color:#0550AE;--shiki-dark:#D4D4D4">18791</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">],</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          autoStart</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">true</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>

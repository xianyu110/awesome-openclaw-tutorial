<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">        },</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        prune</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          idleHours</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#D4D4D4">24</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#6E7781;--shiki-dark:#6A9955">// 0 disables idle pruning</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          maxAgeDays</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#D4D4D4">7</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#6E7781;--shiki-dark:#6A9955">// 0 disables max-age pruning</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">        },</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">      },</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">    },</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">  },</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  tools</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">    sandbox</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>

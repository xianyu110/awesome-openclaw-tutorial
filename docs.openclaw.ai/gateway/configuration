<span class="line"><span style="color:#953800;--shiki-dark:#DCDCAA">openclaw</span><span style="color:#0A3069;--shiki-dark:#CE9178"> gateway</span><span style="color:#0A3069;--shiki-dark:#CE9178"> call</span><span style="color:#0A3069;--shiki-dark:#CE9178"> config.patch</span><span style="color:#0550AE;--shiki-dark:#569CD6"> --params</span><span style="color:#0A3069;--shiki-dark:#CE9178"> &#x27;{</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  &quot;raw&quot;: &quot;{\\n  channels: { telegram: { groups: { \\&quot;*\\&quot;: { requireMention: false } } } }\\n}\\n&quot;,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  &quot;baseHash&quot;: &quot;&lt;hash-from-config.get&gt;&quot;,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  &quot;sessionKey&quot;: &quot;agent:main:whatsapp:dm:+15555550123&quot;,</span></span>

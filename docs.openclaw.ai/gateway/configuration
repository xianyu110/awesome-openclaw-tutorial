<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  channels</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">    whatsapp</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">      accounts</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        default</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {}, </span><span style="color:#6E7781;--shiki-dark:#6A9955">// optional; keeps the default id stable</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        personal</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {},</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        biz</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#6E7781;--shiki-dark:#6A9955">          // Optional override. Default: ~/.openclaw/credentials/whatsapp/biz</span></span>
<span class="line"><span style="color:#6E7781;--shiki-dark:#6A9955">          // authDir: &quot;~/.openclaw/credentials/whatsapp/biz&quot;,</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">        },</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">      },</span></span>

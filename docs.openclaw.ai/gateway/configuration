<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">  discovery</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: { </span><span style="color:#0A3069;--shiki-dark:#CE9178">wideArea</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: { </span><span style="color:#0A3069;--shiki-dark:#CE9178">enabled</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">true</span><span style="color:#1F2328;--shiki-dark:#D4D4D4"> } },</span></span>

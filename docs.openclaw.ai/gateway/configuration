<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">  { </span><span style="color:#0A3069;--shiki-dark:#CE9178">id</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;mueller-docs&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#0A3069;--shiki-dark:#CE9178">workspace</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;~/clients/mueller/docs&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4"> },</span></span>

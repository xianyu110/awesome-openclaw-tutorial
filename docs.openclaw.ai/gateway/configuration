<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">    telegram</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">      enabled</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0550AE;--shiki-dark:#569CD6">true</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">      botToken</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;your-bot-token&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">      dmPolicy</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;pairing&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#6E7781;--shiki-dark:#6A9955">// pairing | allowlist | open | disabled</span></span>

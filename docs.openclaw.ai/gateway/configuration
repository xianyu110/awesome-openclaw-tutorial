<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        scope</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: {</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          default</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;deny&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">,</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">          rules</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [{ </span><span style="color:#0A3069;--shiki-dark:#CE9178">action</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;allow&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">, </span><span style="color:#0A3069;--shiki-dark:#CE9178">match</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: { </span><span style="color:#0A3069;--shiki-dark:#CE9178">chatType</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: </span><span style="color:#0A3069;--shiki-dark:#CE9178">&quot;direct&quot;</span><span style="color:#1F2328;--shiki-dark:#D4D4D4"> } }],</span></span>
<span class="line"><span style="color:#1F2328;--shiki-dark:#D4D4D4">        },</span></span>
<span class="line"><span style="color:#0A3069;--shiki-dark:#CE9178">        models</span><span style="color:#1F2328;--shiki-dark:#D4D4D4">: [</span></span>
